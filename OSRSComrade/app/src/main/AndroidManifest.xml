            </intent-filter>
        </activity>
        <activity
            android:name=".mainScreen"
            android:launchMode="singleTop"
            android:parentActivityName=".mainScreen"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">

